{
  "name": "@quikapp/api-client",
  "version": "1.0.0",
  "description": "TypeScript API client for QuikApp",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "generate": "npm run bundle:openapi && npm run generate:types && npm run generate:client",
    "bundle:openapi": "npx @redocly/cli bundle ../../docs/openapi/openapi.yaml -o src/generated/openapi-bundled.yaml",
    "generate:types": "openapi-typescript src/generated/openapi-bundled.yaml -o src/generated/schema.ts",
    "generate:client": "ts-node --transpile-only scripts/generate-client.ts",
    "build": "npm run generate && tsup",
    "build:only": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rimraf dist src/generated",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "openapi-fetch": "^0.9.0"
  },
  "devDependencies": {
    "@redocly/cli": "^1.25.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "openapi-typescript": "^6.7.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "typescript": ">=4.7"
  },
  "keywords": [
    "quikapp",
    "api",
    "client",
    "typescript",
    "openapi"
  ],
  "author": "QuikApp Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/quikapp/quikapp.git",
    "directory": "packages/api-client"
  },
  "engines": {
    "node": ">=18"
  }
}
