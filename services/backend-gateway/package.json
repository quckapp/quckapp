{
  "name": "quckchat-backend",
  "version": "1.0.0",
  "description": "QuckChat Backend - Real-time chat with audio/video calling",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "test:ci": "cross-env NODE_ENV=test jest --ci --coverage --reporters=default --reporters=jest-junit",
    "test:api": "newman run postman/collection.json -e postman/environment.json",
    "test:api:ci": "wait-on http://localhost:3000/health && newman run postman/collection.json -e postman/environment.json --reporters cli,junit --reporter-junit-export ./reports/newman-results.xml",
    "create:admin": "ts-node src/scripts/create-admin.ts",
    "seed": "ts-node src/scripts/seed.ts",
    "seed:clean": "ts-node src/scripts/seed.ts --clean",
    "migrate:status": "migrate-mongo status",
    "migrate:up": "migrate-mongo up",
    "migrate:down": "migrate-mongo down",
    "migrate:create": "migrate-mongo create",
    "start:gateway": "ts-node src/microservices/gateway/main.ts",
    "start:gateway:dev": "ts-node-dev --respawn src/microservices/gateway/main.ts",
    "start:gateway:prod": "node dist/microservices/gateway/main.js",
    "start:auth": "ts-node src/microservices/auth-service/main.ts",
    "start:auth:dev": "ts-node-dev --respawn src/microservices/auth-service/main.ts",
    "start:auth:prod": "node dist/microservices/auth-service/main.js",
    "start:users": "ts-node src/microservices/users-service/main.ts",
    "start:users:dev": "ts-node-dev --respawn src/microservices/users-service/main.ts",
    "start:users:prod": "node dist/microservices/users-service/main.js",
    "start:messages": "ts-node src/microservices/messages-service/main.ts",
    "start:messages:dev": "ts-node-dev --respawn src/microservices/messages-service/main.ts",
    "start:messages:prod": "node dist/microservices/messages-service/main.js",
    "start:conversations": "ts-node src/microservices/conversations-service/main.ts",
    "start:conversations:dev": "ts-node-dev --respawn src/microservices/conversations-service/main.ts",
    "start:conversations:prod": "node dist/microservices/conversations-service/main.js",
    "start:notifications": "ts-node src/microservices/notifications-service/main.ts",
    "start:notifications:dev": "ts-node-dev --respawn src/microservices/notifications-service/main.ts",
    "start:notifications:prod": "node dist/microservices/notifications-service/main.js",
    "start:media": "ts-node src/microservices/media-service/main.ts",
    "start:media:dev": "ts-node-dev --respawn src/microservices/media-service/main.ts",
    "start:media:prod": "node dist/microservices/media-service/main.js",
    "start:calls": "ts-node src/microservices/calls-service/main.ts",
    "start:calls:dev": "ts-node-dev --respawn src/microservices/calls-service/main.ts",
    "start:calls:prod": "node dist/microservices/calls-service/main.js",
    "start:analytics": "ts-node src/microservices/analytics-service/main.ts",
    "start:analytics:dev": "ts-node-dev --respawn src/microservices/analytics-service/main.ts",
    "start:analytics:prod": "node dist/microservices/analytics-service/main.js",
    "start:all": "concurrently \"npm run start:auth\" \"npm run start:users\" \"npm run start:messages\" \"npm run start:conversations\" \"npm run start:notifications\" \"npm run start:media\" \"npm run start:calls\" \"npm run start:analytics\" \"npm run start:gateway\"",
    "start:all:dev": "concurrently \"npm run start:auth:dev\" \"npm run start:users:dev\" \"npm run start:messages:dev\" \"npm run start:conversations:dev\" \"npm run start:notifications:dev\" \"npm run start:media:dev\" \"npm run start:calls:dev\" \"npm run start:analytics:dev\" \"npm run start:gateway:dev\"",
    "start:services": "concurrently \"npm run start:auth\" \"npm run start:users\" \"npm run start:messages\" \"npm run start:conversations\" \"npm run start:notifications\" \"npm run start:media\" \"npm run start:calls\" \"npm run start:analytics\"",
    "docker:up": "docker-compose -f docker-compose.microservices.yml up -d",
    "docker:down": "docker-compose -f docker-compose.microservices.yml down",
    "docker:build": "docker-compose -f docker-compose.microservices.yml build",
    "docker:logs": "docker-compose -f docker-compose.microservices.yml logs -f",
    "docker:restart": "docker-compose -f docker-compose.microservices.yml restart",
    "docker:infra": "docker-compose -f docker-compose.microservices.yml up -d mongodb redis",
    "wait:db": "wait-on tcp:27017 -t 30000",
    "wait:redis": "wait-on tcp:6379 -t 30000",
    "wait:services": "wait-on tcp:27017 tcp:6379 -t 60000",
    "wait:server": "wait-on http://localhost:3000/health -t 60000",
    "dev:ready": "npm run wait:services && npm run start:dev",
    "ci:test": "npm run wait:services && npm run test:ci",
    "prepare": "husky",
    "commit": "cz",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "docs": "compodoc -p tsconfig.json",
    "docs:serve": "compodoc -p tsconfig.json -s",
    "docs:watch": "compodoc -p tsconfig.json -s -w",
    "docs:coverage": "compodoc -p tsconfig.json --coverageTest 80"
  },
